{
  "name": "jsonschemacycle",
  "version": "0.0.1-alpha",
  "description": "A Javascript library that identifies cycles in complex JSON schema networks",
  "keywords": [
    "json schema cycles graph",
    "first depth search FDS"
  ],
  "author": "Terazus <batistadominique@hotmail.com>",
  "main": "dist/main.js",
  "scripts": {
    "test:unit": "jest",
    "build": "webpack && minify dist/main.js > dist/main.min.js",
    "watch": "webpack --watch",
    "test:bench": "webpack tests/bench/ -o bench/index.js && node bench/",
    "deploy": "jest && npm run test:bench && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "core-js": "^3.6.5",
    "babel-polyfill": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "webpack": "^4.44.2",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-function-bind": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "babel-jest": "26.3.0",
    "jest": "26.4.2",
    "webpack-cli": "^3.3.12",
    "benchmark": "^2.1.4",
    "request": "^2.88.2",
    "lodash": "^4.17.20",
    "sinon": "^9.0.3",
    "axios": "^0.20.0",
    "minify": "^6.0.0"
  }
